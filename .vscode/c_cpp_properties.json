{
  "configurations": [
    {
      "name": "linux-gcc-x64",
      "includePath": ["${workspaceFolder}/**"],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "linux-gcc-x64",
      "compilerArgs": [""]
    },
    {
      "name": "linux-clang-bpf",
      "includePath": [
        "${workspaceFolder}/**",
        "/usr/include",
        "/usr/include/x86_64-linux-gnu",
        "/usr/include/bpf"
      ],
      "defines": ["__TARGET_ARCH_x86"],
      "compilerPath": "/usr/bin/clang",
      "cStandard": "c11",
      "intelliSenseMode": "linux-clang-x64",
      "compilerArgs": ["-target", "bpf", "-O2"]
    }
  ],
  "version": 4
}
